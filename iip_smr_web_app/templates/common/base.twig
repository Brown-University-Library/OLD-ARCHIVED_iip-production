{% load static %}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate"/>
    <meta http-equiv="Pragma" content="no-cache"/>
    <meta http-equiv="Expires" content="0"/>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="theme-color" content="#000000">

    {% block opengraph %}
      <meta property="og:url" content="//library.brown.edu/iip/">
      <meta property="og:site_name" content="A collection of ancient inscriptions
from Israel/Palestine">
      <meta property="og:image" content="https://raw.githubusercontent.com/Brown-University-Library/iip-story-images/master/earthquake/Brooklyn_Museum_-_The_Earthquake_(Le_tremblement_de_terre_)_-_James_Tissot.jpg">
      <meta
        property="og:description"
        content="The Inscriptions of Israel/Palestine project seeks to collect and make freely accessible all of the previously published inscriptions (and their English translations) of Israel/Palestine from the Persian period through the Islamic conquest (ca. 500 BCE - 640 CE). Epigraphy is the study of such inscriptions, defined as texts written on durable materials (except for coins, which falls under the academic category of numismatics).">
      <meta property="og:type" content="website">
    {% endblock %}

    <title>
      {% block title %}{% endblock %}
    </title>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async="async" src="https://www.googletagmanager.com/gtag/js?id=UA-3203647-22"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag('js', new Date());

      gtag('config', 'UA-3203647-22');
    </script>

    <link rel="shortcut icon" href="/static/img/favicon.ico">
    <link rel="stylesheet" href="https://cdn.materialdesignicons.com/4.5.95/css/materialdesignicons.min.css" type="text/css" media="screen"/>

    {% load staticfiles %}

    <link href='{% static "about/css/mobile-menu.css" %}' rel='stylesheet'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    {% block queryCapture %}{% endblock %}

    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.css" rel="stylesheet" type='text/css'>

    <link rel="stylesheet" href='{% static "common/css/system.css" %}'>
    <link rel="stylesheet" href="{% static 'common/css/base.css' %}" type="text/css"/>
    <link rel="stylesheet" href="{% static 'common/css/style.css' %}" type="text/css"/>
    <link rel="stylesheet" href="{% static 'css/dist/styles.css' %}" type="text/css"/>

    {% block extra_css %}{% endblock %}
  </head>

  <body class="{{body_class}}">

    {% block header %}
      {% include "common/header.twig" %}
    {% endblock %}

    {% block body %}{% endblock %}

    {% block content %}{% endblock %}

    {% block viewinscr %}{% endblock %}

    {% block footer %}
      {% include "common/footer.twig" %}
    {% endblock %}

    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    <script async="async" defer="defer" src='{% static 'common/js/mobile-menu.js' %}'></script>
    <script async="async" defer="defer" src='{% static 'common/js/base.js' %}'></script>
    <script async="async" defer="defer" src="{% static 'common/js/site.js' %}" type="text/javascript"></script>

    {% block extra_js %}{% endblock %}

  </body>
</html>
